{
	"name": "FixTar",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"ai:codemap": "node scripts/generate-ai-codemap.js",
		"ai:codemap:check": "node scripts/generate-ai-codemap.js --check",
		"db:setup": "tsx scripts/db/setup-database.ts",
		"db:products": "tsx scripts/db/create-products.ts",
		"db:categories": "tsx scripts/db/create-categories.ts",
		"db:migrate": "drizzle-kit push"
	},
	"type": "module",
	"peerDependencies": {
		"@sveltejs/kit": "^2.0.0",
		"svelte": "^5.0.0"
	},
	"devDependencies": {
		"@eslint/compat": "^2.0.2",
		"@eslint/js": "^10.0.1",
		"@playwright/test": "^1.58.2",
		"@sveltejs/adapter-auto": "^7.0.1",
		"@sveltejs/adapter-netlify": "^6.0.3",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/forms": "^0.5.11",
		"@tailwindcss/postcss": "^4.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@tailwindcss/vite": "^4.2.1",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/svelte": "^5.3.1",
		"@types/node": "^25.3.0",
		"@vitest/browser": "^4.0.18",
		"@vitest/coverage-v8": "^4.0.18",
		"autoprefixer": "^10.4.24",
		"dotenv": "^17.3.1",
		"drizzle-kit": "^0.31.9",
		"eslint": "^10.0.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-svelte": "^3.15.0",
		"globals": "^17.3.0",
		"jsdom": "^28.1.0",
		"playwright": "^1.58.2",
		"postcss": "^8.5.6",
		"prettier": "^3.8.1",
		"prettier-plugin-svelte": "^3.5.0",
		"prettier-plugin-tailwindcss": "^0.7.2",
		"svelte": "^5.53.3",
		"svelte-check": "^4.4.3",
		"tailwindcss": "^4.2.1",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"typescript-eslint": "^8.56.1",
		"vite": "^7.3.1",
		"vitest": "^4.0.18"
	},
	"keywords": [
		"svelte"
	],
	"dependencies": {
		"@libsql/client": "^0.17.0",
		"@sveltejs/kit": "^2.53.0",
		"better-auth": "^1.4.19",
		"drizzle-orm": "^0.45.1",
		"runed": "^0.37.1",
		"zod": "^4.3.6"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"packageManager": "pnpm@9.15.3",
	"description": "This is an e-commerce web application built with SvelteKit, Svelte 5 Runes, simplified for better maintainability.",
	"main": "eslint.config.js",
	"author": "",
	"license": "ISC"
}
